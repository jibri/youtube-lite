{
  "name": "youtube-lite",
  "private": true,
  "version": "3.1.5",
  "type": "module",
  "scripts": {
    "start": "tsc && vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "up-bl": "npx update-browserslist-db@latest",
    "deploy": "yarn && yarn version --patch && git push && yarn build && firebase deploy --only hosting",
    "deploy:major": "yarn && yarn version --major && git push && yarn build && firebase deploy --only hosting",
    "deploy:minor": "yarn && yarn version --minor && git push && yarn build && firebase deploy --only hosting"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/google.accounts": "^0.0.14",
    "@types/jest": "^29.5.11",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-swipe": "^6.0.4",
    "@types/styled-components": "^5.1.34",
    "@types/youtube": "^0.0.50",
    "date-fns": "^3.3.1",
    "firebase": "^10.7.2",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.3",
    "react-swipe": "^6.0.4",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
